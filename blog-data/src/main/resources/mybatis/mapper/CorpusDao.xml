<?xml version="1.0" encoding="UTF-8"?><!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd"><mapper namespace="cn.jxufe.dao.CorpusDao">    <select id="selectExistByCorpusId" resultType="java.lang.Boolean">        select count(*)        from corpus        where id = #{corpusId}        limit 1    </select>    <delete id="deleteByCorpusId">        delete        from corpus        where id = #{corpusId}        limit 1    </delete>    <insert id="insertCorpus">        insert into corpus(name, user_id)            value (#{name}, #{userId});    </insert>    <update id="updateCorpus">        update corpus        set name=#{name}        where id = #{id}        limit 1    </update></mapper>